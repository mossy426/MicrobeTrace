{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "MicrobeTrace": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/MicrobeTrace",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/assets/common/data",
              "src/assets/images/Logo.png",
              "src/Logo.png",
              "src/default.microbetrace",
              "src/map.microbetrace",
              "src/outbreak.microbetrace",
              "src/outbreaknorm.microbetrace",
              "src/outbreakover.microbetrace",
              "src/assets/parse-csv-matrix.js",
              "src/assets/papaparse.min.js"
            ],
            "styles": [

              {
                "input": "src/microbetrace-theme.scss"
              },
              {
                "input": "src/styles.scss"
              },
              {
                "input": "node_modules/primeng/resources/primeng.min.css"
              },
              {
                "input": "node_modules/primeng/resources/themes/saga-blue/theme.css"
              },
              {
                "input": "node_modules/primeicons/primeicons.css"
              },
              {
                "input": "src/assets/metronic/assets/vendors/base/vendors.bundle.css"
              },
              {
                "input": "src/assets/primeng/file-upload/css/primeng.file-upload.css"
              },
              {
                "input": "src/assets/primeng/autocomplete/css/primeng.autocomplete.css"
              },
              {
                "input": "src/assets/primeng/tree/css/primeng.tree.css"
              },
              {
                "input": "src/assets/primeng/context-menu/css/primeng.context-menu.css"
              },
              {
                "input": "node_modules/leaflet/dist/leaflet.css"
              },
              {
                "input": "node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css"
              }
            ],
            "scripts": [
              "src/assets/metronic/assets/vendors/base/vendors.bundle.js",
              "src/assets/metronic/assets/demo/default/base/scripts.bundle.js"
            ]
          },
          "configurations": {
            "development": {
              "optimization": false,
              "sourceMap": false,
              "namedChunks": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": false,
              "watch": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "20mb"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "none",
              "sourceMap": false,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": false,
              "vendorChunk": true,
              "buildOptimizer": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "20mb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "MicrobeTrace:build"
          },
          "configurations": {
            "development": {
              "browserTarget": "MicrobeTrace:build:development"
            },
            "production": {
              "browserTarget": "MicrobeTrace:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "MicrobeTrace:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        }
      }
    },
    "MicrobeTrace-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "MicrobeTrace:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "MicrobeTrace:serve:production"
            }
          }
        }
      }
    }
  }
}